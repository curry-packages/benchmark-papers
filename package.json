{
    "name": "benchmark-papers",
    "version": "2.0.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "A tool to support executable benchmark papers",
    "category": [ "Programming", "Testing" ],
    "dependencies": {
        "base"     : ">= 1.0.0, < 2.0.0",
        "profiling": ">= 0.0.1"
    },
    "compilerCompatibility": {
        "pakcs": ">= 2.0.0",
        "kics2": ">= 2.0.0"
    },
    "exportedModules": [ "Test.Benchmark", "Test.Benchmark.Goodies" ],
    "configModule": "Test.Benchmark.PackageConfig",
    "executable": {
        "name": "exec-benchmarkpaper",
        "main": "ExecuteBenchmarkPaper"
    },
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/benchmark-papers.git",
        "tag": "$version"
    }
}
